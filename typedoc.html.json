{
    "$schema": "https://typedoc.org/schema.json",
    "entryPoints": [
        "src/*.ts",
        "node_modules/@types/aws-lambda/*.d.ts",
        "node_modules/itty-router/types/*.d.ts"
    ],
    "includeVersion": true,
    "out": "docs/typedoc/html/",
    "plugin": [
        "typedoc-plugin-mdn-links",
        "typedoc-github-theme"
    ],
    "sort": [
        "visibility",
        "kind",
        "instance-first",
        "alphabetical-ignoring-documents"
    ],
    "theme": "typedoc-github-theme",
}
